{
  "name": "playwright-cli-automation",
  "version": "1.0.1",
  "description": "CLI browser automation with Playwright - Firefox, stealth, proxy, parallel execution",
  "main": "src/index.js",
  "bin": {
    "autobot": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "health": "node src/health/server.js",
    "docker:build": "docker build -t autobot:latest .",
    "docker:build-prod": "docker build -t lalalaala/playwright-cli-automation:latest .",
    "docker:push": "./deploy/docker-deploy.sh",
    "docker:deploy": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:test": "./tests/acceptance/run-acceptance-tests.sh",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unit": "node --experimental-vm-modules node_modules/.bin/jest tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/.bin/jest tests/integration",
    "test:acceptance": "./tests/acceptance/run-acceptance-tests.sh",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs:check": "node tai-lieu-du-an/scripts/doc-consistency-check/check.js",
    "prepublishOnly": "npm test",
    "prepare": "husky || true"
  },
  "keywords": [
    "playwright",
    "browser-automation",
    "cli",
    "web-scraping",
    "stealth"
  ],
  "author": "Your Name <email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lizamazieva41-ai/playwright-cli-automation.git"
  },
  "homepage": "https://github.com/lizamazieva41-ai/playwright-cli-automation#readme",
  "bugs": {
    "url": "https://github.com/lizamazieva41-ai/playwright-cli-automation/issues"
  },
  "files": [
    "src/",
    "config/",
    "examples/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "dependencies": {
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "nodemailer": "^8.0.1",
    "p-limit": "3.1.0",
    "playwright": "^1.40.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
